@model IEnumerable<EjemploRazor.Models.MADRID>

@{
    ViewBag.Title = "Plantilla";
}

<h2>Plantilla</h2>
<table class="table">
    <tr>
        
        <th>@Html.DisplayNameFor(model => model.DORSAL)</th>
        <th>@Html.DisplayNameFor(model => model.POSICION)</th>
        <th>@Html.DisplayNameFor(model => model.NOMBRE)</th>
        <th>@Html.DisplayNameFor(model => model.PAIS)</th>
        <th>@Html.DisplayNameFor(model => model.FECHA_NACIMIENTO)</th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>@item.DORSAL</td>
        <td>@Html.DisplayFor(modelItem => item.POSICION)</td>
        <td>@Html.DisplayFor(modelItem => item.NOMBRE)</td>
        <td>@Html.DisplayFor(modelItem => item.PAIS)</td>
        <td>@Html.DisplayFor(modelItem => item.FECHA_NACIMIENTO)</td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Detalles Jugador", "Details", new { dorsal = item.DORSAL }) |
            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            
        </td>
    </tr>
}

</table>
